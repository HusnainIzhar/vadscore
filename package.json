{
  "name": "vadscore",
  "version": "1.0.0",
  "description": "A high-accuracy Voice Activity Detection (VAD) library for React with real-time scoring and speech detection",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vad",
    "voice-activity-detection",
    "speech-detection",
    "audio",
    "react",
    "typescript",
    "real-time"
  ],
  "author": "Husnain Izhar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/husnainizhar/vadscore.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jsdom": "^26.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^3.20.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  }
}
